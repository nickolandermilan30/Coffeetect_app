<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img_3"
    tools:context=".Othe_tree">

    <Button
        android:id="@+id/saveButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="88dp"
        android:text="Save EditText"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="119dp"
        android:layout_height="97dp"
        android:fontFamily="@font/hammersmith_one"
        android:text="Other Trees"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082" />

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="50dp"
        android:layout_height="51dp"
        android:background="@drawable/back"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/t7"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/btn1"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.64" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t7"
        app:layout_constraintEnd_toEndOf="@+id/t7"
        app:layout_constraintStart_toStartOf="@+id/t7"
        app:layout_constraintTop_toTopOf="@+id/t7"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView13"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:gravity="center"
        android:inputType="text"
        android:hint="Tree 7"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t7"
        app:layout_constraintEnd_toEndOf="@+id/t7"
        app:layout_constraintStart_toStartOf="@+id/t7"
        app:layout_constraintTop_toTopOf="@+id/t7"
        app:layout_constraintVertical_bias="0.514" />





    <ImageButton
        android:id="@+id/t8"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/btn3"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.936"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t8"
        app:layout_constraintEnd_toEndOf="@+id/t8"
        app:layout_constraintStart_toStartOf="@+id/t8"
        app:layout_constraintTop_toTopOf="@+id/t8"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView14"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:gravity="center"
        android:inputType="text"
        android:hint="Tree 8"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t8"
        app:layout_constraintEnd_toEndOf="@+id/t8"
        app:layout_constraintStart_toStartOf="@+id/t8"
        app:layout_constraintTop_toTopOf="@+id/t8"
        app:layout_constraintVertical_bias="0.514" />

    <ImageButton
        android:id="@+id/t9"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:background="@drawable/btn2"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t7"
        app:layout_constraintVertical_bias="0.141" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t9"
        app:layout_constraintEnd_toEndOf="@+id/t9"
        app:layout_constraintStart_toStartOf="@+id/t9"
        app:layout_constraintTop_toTopOf="@+id/t9"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView15"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:gravity="center"
        android:inputType="text"
        android:hint="Tree 9"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t9"
        app:layout_constraintEnd_toEndOf="@+id/t9"
        app:layout_constraintStart_toStartOf="@+id/t9"
        app:layout_constraintTop_toTopOf="@+id/t9"
        app:layout_constraintVertical_bias="0.514" />


    <ImageButton
        android:id="@+id/t10"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:background="@drawable/btn6"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.936"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t7"
        app:layout_constraintVertical_bias="0.141" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t10"
        app:layout_constraintEnd_toEndOf="@+id/t10"
        app:layout_constraintStart_toStartOf="@+id/t10"
        app:layout_constraintTop_toTopOf="@+id/t10"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView16"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:gravity="center"
        android:inputType="text"
        android:hint="Tree 10"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t10"
        app:layout_constraintEnd_toEndOf="@+id/t10"
        app:layout_constraintStart_toStartOf="@+id/t10"
        app:layout_constraintTop_toTopOf="@+id/t10"
        app:layout_constraintVertical_bias="0.514" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:background="@drawable/login_white_backg7"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints" />


    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/home"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector" />

        <ImageButton
            android:id="@+id/leaf"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector2"

            />

        <ImageButton
            android:id="@+id/cam"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector3"

            />


        <ImageButton
            android:id="@+id/history"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector4"
            android:backgroundTint="#01BB7D" />

        <ImageButton
            android:id="@+id/cal"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector5" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>